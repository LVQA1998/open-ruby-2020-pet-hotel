<div class="featured-wrap">
  <div class="heading-title"><%= t "list_all_job_html" %></div>
  <div class="container">
    <%= render "posts/search" %>
    <div><%= @feed_items.total_count %> <%= t "post_found" %></div>
    <% if @feed_items.any? %>
      <ol class="row">
        <%= render @feed_items %>
      </ol>
      <%= paginate @feed_items %>
    <% end %>
  </div>
</div>
